    "change-case": "^5.4.2",
    "classnames": "^2.5.1",
    "crc": "^4.3.2",
    "date-fns": "^3.2.0",
    "deepmerge": "^4.3.1",
    "electron-updater": "^6.1.1",
    "fast-deep-equal": "^3.1.3",
    "fast-sort": "^3.4.0",
    "fetch-retry": "^5.0.6",
    "framer-motion": "^9.0.2",
    "fuse.js": "^7.0.0",
    "immer": "^10.0.3",
    "jotai": "^2.6.1",
    "jotai-immer": "^0.2.0",
    "jotai-location": "^0.5.2",
    "lodash": "^4.17.21",
    "mime": "^4.0.1",
    "nanoevents": "^9.0.0",
    "node-html-parser": "^6.1.12",
    "nodejs-file-downloader": "^4.12.1",
    "react-flip-toolkit": "^7.1.0",
